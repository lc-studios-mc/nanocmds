{
	"name": "nanocmds",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsx scripts/build.ts",
		"build:dev": "dotenv -v DEV=1 -- tsx scripts/build.ts",
		"build:dev:watch": "dotenv -v DEV=1 -v WATCH=1 -- tsx scripts/build.ts",
		"clean": "shx rm -rf build",
		"typecheck": "tsc && tsc -p scripts/tsconfig.json",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@minecraft/server": "2.2.0",
		"@minecraft/server-ui": "2.0.0"
	},
	"devDependencies": {
		"@mcbe-toolbox-lc/builder": "^0.1.1",
		"@types/archiver": "^6.0.3",
		"@types/node": "^22.17.0",
		"archiver": "^6.0.2",
		"dotenv-cli": "^11.0.0",
		"prettier": "^3.6.2",
		"sync-directory": "^6.0.5",
		"typescript": "^5.9.2"
	}
}
